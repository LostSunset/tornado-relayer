    "ethers": "^5.6.4",
    "fastify": "^3.28.0",
    "fastify-cors": "^6.0.3",
    "fixed-merkle-tree": "^0.7.3",
    "gas-price-oracle": "^0.3.5",
    "ioredis": "^4.14.1",
    "json-schema-to-ts": "^2.2.0",
    "node-fetch": "^2.6.7",
